<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:fitsSystemWindows="true"
    tools:context="com.portal.full.appmozoou.ViewNews"
  >

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:id="@+id/editTextId"
        android:visibility="gone" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:id="@+id/editTextName"
        android:textStyle="bold"
        android:textSize="20dp"
        android:textIsSelectable="false" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="19dp"
        android:weightSum="1"
        android:visibility="gone">

        <TextView
            android:id="@+id/star"
            android:layout_width="114dp"
            android:layout_height="wrap_content"
            android:textColor="@color/colorIconAction"
            android:minHeight="30dp"
            android:textSize="@dimen/size_textview"
            android:layout_gravity="left"
            android:text="@string/star"
            android:layout_weight="0.35"
            android:visibility="gone" />

        <TextView
            android:id="@+id/lablestar"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:textColor="@color/colorIconAction"
            android:minHeight="30dp"
            android:layout_gravity="right"
            android:textSize="@dimen/size_textview"
            android:text="@string/lablestar"
            android:visibility="gone" />

    </LinearLayout>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editTextDesg"
        android:visibility="gone" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:id="@+id/editTextIdad" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:id="@+id/editTextTarget" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:id="@+id/editTextBanner" />


        <WebView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/webView"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_weight="1.15"
            android:layout_gravity="right" />

    <WebView
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:id="@+id/adv"
            />


</LinearLayout>
