<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior" tools:showIn="@layout/app_bar_main"
    tools:context="com.portal.full.appmozoou.Main"

    android:scrollIndicators="start"
    android:layout_marginTop="@dimen/cardview_default_radius"
    android:background="@drawable/list_selector"
    >
    <WebView
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/txtmoive"
        android:layout_alignParentTop="false"
        android:layout_alignParentStart="false"

        android:background="#303F9F"
        android:backgroundTint="#303F9F"
        android:clickable="false" />

    <ListView
        android:id="@+id/list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"

        android:dividerHeight="2dp"

        android:layout_marginTop="40dp"
        android:listSelector="@drawable/list_selector">
    </ListView>



</RelativeLayout>
